"MY EDIT
set guifont=Consolas:h18
syntax on
colorscheme molokai
set nu
set ai
set cursorline
set expandtab
set tabstop=4
set incsearch
set mouse=""
set backspace=2
set shiftwidth=4
set formatoptions+=r
set history=100
set undodir=~/.vim/undo
set undofile
set undolevels=1000
set undoreload=10000
inoremap ( ()<Esc>i
inoremap " ""<Esc>i
inoremap ' ''<Esc>i
inoremap [ []<Esc>i
inoremap {<CR> {<CR>}<Esc>ko
inoremap {{ {}<ESC>i
filetype indent on
filetype indent plugin on
set wildmenu
set ignorecase
set smartcase
set smarttab
